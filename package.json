{
  "name": "wire-account",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "babel": "npm run -s babel-extract && npm run -s babel-update && npm run -s babel-compile",
    "babel-compile": "pybabel compile -f -d app/translations",
    "babel-extract":
      "pybabel extract -k _ -k __ -F app/translations/babel.cfg --sort-by-file --omit-header -o app/translations/account.pot app",
    "babel-init": "pybabel init -i app/translations/account.pot -d app/translations -l",
    "babel-update": "pybabel update -i app/translations/account.pot -d app/translations --no-wrap",
    "build": "gulp build",
    "crowdin":
      "npm run -s babel && crowdin-cli --identity=keys/crowdin.yaml upload sources && crowdin-cli --identity=keys/crowdin.yaml download && npm run -s babel && crowdin-cli --identity=keys/crowdin.yaml download",
    "postinstall": "pip install --user -r app/requirements.txt",
    "start": "concurrently \"gulp\" \"python app/application.py\"",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wireapp/wire-account.git"
  },
  "dependencies": {
    "normalize.css": "7.0.0",
    "jquery": "3.2.1"
  },
  "devDependencies": {
    "coffee-script": "1.12.7",
    "concurrently": "3.5.0",
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-concat": "2.6.1",
    "gulp-cssnano": "2.1.3",
    "gulp-help": "1.6.1",
    "gulp-less": "3.3.2",
    "gulp-livereload": "3.8.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-plumber": "1.1.0",
    "gulp-sequence": "0.4.6",
    "gulp-size": "2.1.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "gulp-zip": "4.1.0",
    "less": "2.7.3",
    "uglify-js": "3.4.3"
  },
  "private": true
}
