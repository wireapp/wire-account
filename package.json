{
  "dependencies": {
    "@wireapp/api-client": "8.0.0",
    "@wireapp/commons": "2.2.23",
    "@wireapp/react-ui-kit": "7.23.18",
    "core-js": "3.3.4",
    "dotenv": "8.2.0",
    "dotenv-extended": "2.4.0",
    "i18next": "18.0.1",
    "i18next-browser-languagedetector": "4.0.0",
    "i18next-react-postprocessor": "3.0.1",
    "intersection-observer": "0.7.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-i18next": "11.0.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "url-search-params-polyfill": "7.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@emotion/babel-preset-css-prop": "10.0.22",
    "@hot-loader/react-dom": "16.10.2",
    "@types/i18next": "13.0.0",
    "@types/jasmine": "3.4.6",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-router-dom": "4.3.5",
    "@types/webpack-env": "1.14.1",
    "@wireapp/copy-config": "0.8.6",
    "@wireapp/eslint-config": "1.2.0",
    "@wireapp/tslint-config": "1.4.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-remove-jsx-attributes": "0.0.2",
    "cross-env": "6.0.3",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-jasmine": "2.10.1",
    "eslint-plugin-jsdoc": "15.12.1",
    "eslint-plugin-no-unsanitized": "3.0.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "fs-extra": "8.1.0",
    "husky": "3.0.9",
    "i18next-scanner": "2.10.3",
    "jasmine": "3.5.0",
    "lint-staged": "9.4.2",
    "prettier": "1.19.1",
    "react-hot-loader": "4.12.15",
    "svg-inline-loader": "0.8.0",
    "terser-webpack-plugin": "2.2.1",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.1.0",
    "tslint-react-hooks": "2.2.1",
    "typescript": "3.6.4",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-middleware": "3.7.2",
    "webpack-env": "0.8.0",
    "webpack-hot-middleware": "2.25.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "GPL-3.0",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,less,md,yml}": [
      "prettier --write",
      "git add"
    ],
    "src/script/**/*.{ts,tsx}": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ],
    "server/**/*.{ts,tsx}": [
      "cd server && tslint --config server/tslint.json",
      "git add"
    ]
  },
  "name": "wire-account",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/wireapp/wire-account.git"
  },
  "scripts": {
    "build:server": "cd server && yarn build",
    "bundle:prod": "yarn build:server && cross-env-shell NODE_ENV=production yarn clean && yarn webpack && node bin/version_file.js && node bin/deployment_archive.js",
    "bundle:staging": "yarn build:server && cross-env-shell BABEL_ENV=development NODE_ENV=production yarn clean && yarn webpack && node bin/version_file.js && node bin/deployment_archive.js",
    "clean": "exit 0",
    "docker": "node bin/push_docker.js",
    "fix:js": "yarn lint:js --fix",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "fix": "yarn fix:other && yarn fix:ts && yarn fix:js",
    "lint:js": "eslint --ignore-path .gitignore --ignore-path .eslintignore --ext .js .",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json \"**/*.ts\"",
    "lint": "yarn lint:js && yarn lint:ts && yarn lint:other",
    "postinstall": "copy-config && cd server && yarn",
    "postversion": "git push && git push --tags",
    "prettier": "prettier \"**/*.{json,less,md,yml}\"",
    "preversion": "yarn test",
    "start": "cd server && yarn start",
    "test": "yarn test:types",
    "test:types": "tsc --noEmit",
    "translate:extract": "yarn bundle:staging && i18next-scanner 'server/dist/**/*.{js,html}'",
    "translate:upload": "yarn translate:extract && node bin/translations_upload.js",
    "webpack": "webpack --config webpack.config.prod.js",
    "webpack:dev": "webpack --config webpack.config.dev.js",
    "webpack:test": "webpack --config webpack.config.test.js"
  },
  "version": "0.1.0"
}
