{
  "name": "wire-account",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "postinstall": "pip install -r app/requirements.txt",
    "start": "concurrently \"gulp\" \"python app/application.py\"",
    "babel-init": "pybabel init -i app/translations/account.pot -d app/translations -l",
    "babel-extract": "pybabel extract -k _ -k __ -F app/translations/babel.cfg --sort-by-file --omit-header -o app/translations/account.pot app",
    "babel-update": "pybabel update -i app/translations/account.pot -d app/translations --no-wrap",
    "babel-compile": "pybabel compile -f -d app/translations",
    "babel": "npm run -s babel-extract && npm run -s babel-update && npm run -s babel-compile",
    "crowdin": "npm run -s babel && crowdin-cli --identity=keys/crowdin.yaml upload sources && crowdin-cli --identity=keys/crowdin.yaml download && npm run -s babel && crowdin-cli --identity=keys/crowdin.yaml download"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wireapp/wire-account.git"
  },
  "dependencies": {
    "normalize.css": "4.2.0",
    "jquery": "3.1.0"
  },
  "devDependencies": {
    "coffee-script": "1.10.0",
    "concurrently": "3.5.0",
    "del": "2.2.2",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-concat": "2.6.0",
    "gulp-cssnano": "2.1.2",
    "gulp-help": "1.6.1",
    "gulp-less": "3.1.0",
    "gulp-livereload": "3.8.1",
    "gulp-load-plugins": "1.3.0",
    "gulp-plumber": "1.1.0",
    "gulp-sequence": "0.4.6",
    "gulp-size": "2.1.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "2.0.0",
    "gulp-util": "3.0.7",
    "gulp-zip": "3.2.0",
    "less": "2.7.1",
    "uglify-js": "2.7.3"
  },
  "private": true
}
