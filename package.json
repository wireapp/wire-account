    "gulp-uglify": "3.0.1",
    "gulp-util": "3.0.8",
    "gulp-zip": "4.2.0",
    "i18next-scanner": "2.6.6",
    "jasmine": "3.3.0",
    "husky": "1.2.0",
    "less": "3.8.1",
    "nodemon": "1.18.6",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.1.3",
    "lint-staged": "8.1.0",
    "prettier": "1.15.2",
    "uglify-js": "3.4.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "LicenseRef-LICENSE",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,less,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "name": "wire-account",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/wireapp/wire-account.git"
  },
  "scripts": {
    "build": "yarn build:server && yarn build:front",
    "build:front": "gulp build",
    "build:server": "yarn lint:ts --fix && yarn tsc -P ./tsconfig.json",
    "translate": "yarn translate:extract && yarn translate:upload && yarn translate:download",
    "translate:extract": "yarn build && i18next-scanner 'dist/**/*.{js,html}'",
    "translate:download": "node bin/translations_download.js",
    "translate:upload": "yarn translate:extract && node bin/translations_upload.js",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json \"**/*.ts\"",
    "start": "yarn build:server && concurrently \"gulp watch\" \"yarn build:server --w\" \"nodemon ./dist/index.js\"",
    "start:prod": "pm2 start \"./index.js\" --name \"Wire Account Management\" && pm2 logs",
    "test": "yarn build:server && yarn test:server",
    "test:server": "jasmine --config=jasmine.json"
  }
}
